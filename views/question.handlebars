
<div class="container">
  {{#each question}}
           <!-- Card -->
  <div class="card mb-4">

    <!--Card image-->
    <div class="view overlay">
      <img class="card-img-top" src={{this.photo_url}}
        alt="Card image cap">
      <a href="#!">
        <div class="mask rgba-white-slight"></div>
      </a>
    </div>

    <!--Card content-->
    <div class="card-body">

      <!--Title-->
      <h4 class="card-title">how do i pass in category here</h4>
      <!--Text-->
      <p class="card-text"> {{this.question_string}} </p>
      <!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
      <button type="button" class="btn btn-light-blue btn-md">Mark as Done</button>
      <button type="button" class="btn btn-light-blue btn-md">Favorite</button>

    </div>

  </div>
  <!-- Card -->
  {{/each}}</div>